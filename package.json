{
  "private": true,
  "name": "nextjs-routes-development",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tatethurston/nextjs-routes"
  },
  "type": "module",
  "scripts": {
    "build:commonjs": "webpack",
    "build:module": "tsc",
    "clean": "rm -rf dist",
    "e2e:setup": "npm run package:build && (cd e2e && pnpm next build)",
    "examples:each": "for e in ./examples/*; do (cd \"$e\" && $npm_config_command); done",
    "examples:regen": "npm run examples:each --command='pnpm next build'",
    "lint": "npm run typecheck && prettier --check . && prettier-package-json --list-different '{,public.,e2e/,example/}package.json' && eslint .",
    "lint:fix": "prettier --write '**/*.{ts,cts,mts,tsx,md,json,yaml}' && prettier-package-json --write '{,public.,e2e/,example/}package.json' && eslint --fix .",
    "package:build": "npm run clean && npm run build:module && npm run build:commonjs && npm run package:prune && npm run package:copy:files",
    "package:copy:files": "cp ./LICENSE ./README.md dist/ && cp ./public.package.json dist/package.json",
    "package:prune": "find dist -name *.test.* | xargs rm -f",
    "prepare": "husky install",
    "test": "npm run e2e:setup && jest",
    "test:ci": "npm run test --coverage",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@swc/core": "^1.3.46",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-loader": "^9.1.2",
    "chokidar": "^3.5.3",
    "codecov": "^3.8.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "next": "^13.3.0",
    "prettier": "^2.8.7",
    "prettier-package-json": "^2.8.0",
    "react": "^18.2.0",
    "swc-loader": "^0.2.3",
    "typescript": "^5.0.4",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1"
  },
  "packageManager": "pnpm@7.17.0"
}
