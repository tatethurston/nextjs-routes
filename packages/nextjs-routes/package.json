{
  "name": "nextjs-routes",
  "version": "2.0.0",
  "description": "Type safe routing for Next.js",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tatethurston/nextjs-routes"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./config": {
      "require": "./dist/config.cjs",
      "default": "./dist/config.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "nextjs-routes": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc && webpack && chmod +x dist/cli.js",
    "clean": "rm -rf dist",
    "package:build": "pnpm run clean && pnpm run build",
    "package:prune": "find dist -name *.test.* | xargs rm -f"
  },
  "sideEffects": false,
  "dependencies": {
    "chokidar": "^3.5.3"
  },
  "peerDependencies": {
    "next": "*"
  },
  "keywords": [
    "link",
    "next",
    "nextjs",
    "route",
    "routing",
    "type safe",
    "typescript"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  }
}
