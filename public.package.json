{
  "name": "nextjs-routes",
  "version": "0.0.10",
  "description": "Type safe routing for Next.js",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tatethurston/nextjs-routes"
  },
  "bin": {
    "nextjs-routes": "cli.js"
  },
  "sideEffects": false,
  "types": "index.d.ts",
  "peerDependencies": {
    "@types/react": "*",
    "next": "*",
    "prettier": "^2.1.1"
  },
  "keywords": [
    "link",
    "next",
    "nextjs",
    "route",
    "type safe",
    "typescript"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./router": {
      "import": "./module/router.js",
      "module": "./module/router.js",
      "default": "./router.js"
    },
    "./link": {
      "import": "./module/link.js",
      "module": "./module/link.js",
      "default": "./link.js"
    }
  }
}
