{
  "name": "nextjs-routes",
  "version": "0.0.22",
  "description": "Type safe routing for Next.js",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tatethurston/nextjs-routes"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./package.json": "./package.json",
    "./next-config.cjs": {
      "import": {
        "types": "./next-config.d.cts",
        "default": "./next-config.cjs"
      },
      "require": {
        "types": "./next-config.d.cts",
        "default": "./next-config.cjs"
      }
    }
  },
  "bin": {
    "nextjs-routes": "cli.js"
  },
  "sideEffects": false,
  "types": "index.d.ts",
  "dependencies": {
    "chokidar": "^3.5.3"
  },
  "peerDependencies": {
    "next": "*"
  },
  "keywords": [
    "link",
    "next",
    "nextjs",
    "route",
    "type safe",
    "typescript"
  ]
}
